{
  "manifest_version": 3,
  "name": "Your Extension",
  "version": "1.0",
  "background": {
    "service_worker": "background/messageHandler.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "downloads",
    "contextMenus",
    "storage"
  ]
}